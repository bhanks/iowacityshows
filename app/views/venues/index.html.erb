<div data-role="page" data-theme="d">
	<div data-role="header" data-theme="d">
		<h1>Venues</h1>
	</div>
	
	<div data-role="content" date-theme="d">
		<ul data-role="listview">
		<% @venues.each do |venue| %>
		
			<li>
				<h1><%= link_to(venue.name, venue_path(venue.id)) %></h1>
				<p><%= venue.address %></p>
			</li>
		
		<% end %>
		</ul>
	<div>
</div>